{
    "_config_name": "feeds.feeds_importer.github_issues",
    "id": "github_issues",
    "disabled": false,
    "config": {
        "name": "GitHub Issues",
        "description": "Import GitHub Issues",
        "fetcher": {
            "plugin_key": "FeedsHTTPFetcher",
            "config": {
                "auto_detect_feeds": false,
                "use_pubsubhubbub": false,
                "designated_hub": "",
                "request_timeout": null,
                "auto_scheme": "http",
                "accept_invalid_cert": false,
                "cache_http_result": true
            }
        },
        "parser": {
            "plugin_key": "FeedsJSONPathParser",
            "config": {
                "context": "*",
                "sources": {
                    "jsonpath_parser:0": "id",
                    "jsonpath_parser:1": "title",
                    "jsonpath_parser:2": "body",
                    "jsonpath_parser:3": "html_url"
                },
                "debug": {
                    "options": {
                        "context": 0,
                        "jsonpath_parser:0": 0,
                        "jsonpath_parser:1": 0,
                        "jsonpath_parser:2": 0,
                        "jsonpath_parser:3": 0
                    }
                },
                "allow_override": 0,
                "convert_four_byte": 0
            }
        },
        "processor": {
            "plugin_key": "FeedsNodeProcessor",
            "config": {
                "expire": "-1",
                "author": 0,
                "authorize": 0,
                "mappings": [
                    {
                        "source": "jsonpath_parser:0",
                        "target": "field_github_id",
                        "unique": 1
                    },
                    {
                        "source": "jsonpath_parser:1",
                        "target": "title",
                        "unique": false,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:2",
                        "target": "body",
                        "unique": false,
                        "language": "und"
                    },
                    {
                        "source": "jsonpath_parser:3",
                        "target": "field_github_link:url",
                        "unique": false,
                        "language": "und"
                    }
                ],
                "insert_new": "1",
                "update_existing": "2",
                "update_non_existent": "unpublish",
                "input_format": "filtered_html",
                "skip_hash_check": 0,
                "bundle": "feature",
                "language": "und"
            }
        },
        "content_type": "",
        "update": 0,
        "import_period": 1800,
        "expire_period": 3600,
        "import_on_create": true,
        "process_in_background": false
    }
}
